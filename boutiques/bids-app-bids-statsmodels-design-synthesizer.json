{
    "command-line": "mkdir -p OUTPUT_DIR; /run.py BIDS_DIR OUTPUT_DIR ANALYSIS_LEVEL PARTICIPANT_LABEL SESSION_LABEL",
    "container-image": {
        "image": "bids/bids-statsmodels-design-synthesizer",
        "type": "docker"
    },
    "description": "See https://github.com/BIDS-Apps/bids-statsmodels-design-synthesizer",
    "groups": [
    ],
    "inputs": [
        {
            "description": "The directory with the input dataset formatted according to the BIDS standard.",
            "id": "bids_dir",
            "name": "BIDS directory",
            "optional": false,
            "type": "File",
            "value-key": "BIDS_DIR"
        },
       {
            "description": "Directory containing the output of the participants analysis.",
            "id": "participant_level_analysis_dir",
            "name": "Participants dir",
            "optional": true,
            "type": "File",
            "value-key": "OUTPUT_DIR"
        },
       {
            "command-line-flag": "--model",
            "description": "The name of the model json (this must be placed in the models directory of the bids data directory)",
            "id": "model",
            "list": false,
            "name": "Model",
            "optional": false,
            "type": "String",
            "value-key": "MODEL"
        },
       {
            "command-line-flag": "--events-tsv",
            "description": "The path to the events-tsv that serves as input for model transformations",
            "id": "events_tsv",
            "list": false,
            "name": "Events_tsv",
            "optional": false,
            "type": "String",
            "value-key": "EVENTS_TSV"
        },
       {
            "command-line-flag": "--output-tsv",
            "description": "The path to the output-tsv that serves as input for model transformations",
            "id": "output_tsv",
            "list": false,
            "name": "output_tsv",
            "optional": true,
            "type": "String",
            "value-key": "OUTPUT_TSV"
        },
        {
            "command-line-flag": "--output-sampling-rate",
            "description": "Output sampling rate when dense representation is used (float specifying Hz).",
            "id": "output sampling rate",
            "list": false,
            "name": "output sampling rate",
            "optional": true,
            "type": "String",
            "value-key": "OUTPUT_SAMPLING_RATE"
        },
        {
            "command-line-flag": "--duration"
            "description": "The duration in seconds of the run",
            "id": "duration",
            "list": false,
            "name": "duration",
            "optional": false,
            "type": "String",
            "value-key": "DURATION"
        },
        {
            "command-line-flag": "--participant_label",
            "description": "The label(s) of the participant(s) that should be analyzed. The label corresponds to sub-<participant_label> from the BIDS spec (so it does not include \"sub-\"). If this parameter is not provided all subjects will be analyzed. Multiple participants can be specified with a space separated list.",
            "id": "participant_label",
            "list": true,
            "name": "Participant label",
            "optional": true,
            "type": "String",
            "value-key": "PARTICIPANT_LABEL"
        },
        {
            "command-line-flag": "--session_label",
            "description": "The label(s) of the session(s) that should be analyzed. The label corresponds to ses-<session_label>, an extension of the BIDS spec (so it does not include \"ses-\"). If this parameter is not provided all sessions will be analyzed. Multiple sessions can be specified with a space separated list.",
            "id": "session_label",
            "list": true,
            "name": "Session label",
            "optional": true,
            "type": "String",
            "value-key": "SESSION_LABEL"
        }
    ],
    "invocation-schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "additionalProperties": false,
        "allOf": [
            {
                "anyOf": [
                    {
                        "required": [
                            "output_dir_name"
                        ]
                    },
                    {
                        "required": [
                            "participant_level_analysis_dir"
                        ]
                    }
                ]
            }
        ],
        "dependencies": {
            "output_dir_name": {
                "properties": {
                    "participant_level_analysis_dir": {
                        "not": {}
                    }
                }
            },
            "participant_level_analysis_dir": {
                "properties": {
                    "output_dir_name": {
                        "not": {}
                    }
                }
            }
        },
        "description": "Invocation schema for bids-statsmodels-design-synthesizer.",
        "properties": {
            "analysis_level": {
                "enum": [
                    "participant",
                    "group",
                    "session"
                ]
            },
            "bids_dir": {
                "type": "string"
            },
            "output_dir_name": {
                "type": "string"
            },
            "participant_label": {
                "items": {
                    "type": "string"
                },
                "type": "array"
            },
            "participant_level_analysis_dir": {
                "type": "string"
            },
            "session_label": {
                "items": {
                    "type": "string"
                },
                "type": "array"
            }
        },
        "required": [
            "bids_dir",
            "analysis_level"
        ],
        "title": "bids-statsmodels-design-synthesizer.invocationSchema",
        "type": "object"
    },
    "name": "bids-statsmodels-design-synthesizer",
    "output-files": [
        {
            "description": "The directory where the output files should be stored. If you are running a group level analysis, this folder should be prepopulated with the results of the participant level analysis.",
            "id": "output_dir",
            "name": "Output directory",
            "optional": false,
            "path-template": "OUTPUT_DIR"
        }
    ],
    "schema-version": "0.5",
    "tool-version": "dev"
}
